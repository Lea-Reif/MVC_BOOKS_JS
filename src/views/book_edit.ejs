
 <% include partials/header%>


 <div class="container mt-5">
  <div class="row">
    <div class="col-md-5 offset-md-3 card">
      <div class="card-body">
        <form action="/update/<%= data.ID %>" method="post">
          <div class="form-group">
            <input type="text" name="names" value="<%= data.NAMES%>" placeholder="Name" class="form-control" autofocus>
          </div>
          <div class="form-group">
            <input type="text" name="isbn" value="<%= data.ISBN%>" placeholder="Address" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="genre" value="<%= data.GENRE%>" placeholder="Phone" class="form-control">
          </div>
          <div class="form-group">  
            <input type="date" name="dates" value="<%= data.DATES%>" placeholder="Phone" class="form-control">
          </div>

          <button type="submit" class="btn btn-primary">
            save book
          </button>
        </form>
      </div>
    </div>
  </div>
</div>
  <% include partials/footer%>
